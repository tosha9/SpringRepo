<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byType" default-autowire-candidates="consoleWritter,fileWritter">

	<bean id="logger" class="autowiring.logger"></bean>
	
	<bean id="consoleWritter" class="autowiring.ConsoleWritter"></bean>
	<bean id="consoleWritterOther" class="autowiring.ConsoleWritter"></bean>
	<bean id="fileWritter" class="autowiring.FileWritter"></bean>
</beans>
